#! /bin/sh -

. ../lib.sh

set -eu

lib_info 'setup'

FILES="$(cat <<-EOF
0mp-explore
cflowfilter
disable-yoga-3-14-touchscreen
faf
fitqemu
samsung2043nw
superdrive
EOF
)"
PREFIX="$HOME/.local/bin/"
lib_create_prefix_if_missing
lib_back_up
trap lib_roll_back EXIT
lib_install
trap - EXIT

FILES="$(cat <<-EOF
0mp-explore.1
EOF
)"
PREFIX="$HOME/.local/share/man/man1/"
lib_create_prefix_if_missing
lib_back_up
trap lib_roll_back EXIT
lib_install
trap - EXIT

lib_info remember to add ~/.local/bin to your PATH

lib_info 'setup done'
