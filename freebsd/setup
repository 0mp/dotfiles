#! /bin/sh -

. ../lib.sh

set -eu

lib_info 'setup'

FILES='fonts.conf'
PREFIX="$HOME/.config/fontconfig/"
lib_create_prefix_if_missing
lib_back_up
trap lib_roll_back EXIT
lib_install
trap - EXIT

FILES='fr justmount unitek-init wifi-init'
PREFIX="$HOME/.local/bin/"
lib_create_prefix_if_missing
lib_back_up
trap lib_roll_back EXIT
lib_install
trap - EXIT

lib_info 'setup done'
